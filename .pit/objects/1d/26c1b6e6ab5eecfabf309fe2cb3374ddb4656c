#ifndef INIT_H
#define INIT_H

#include <iostream>
#include "repository.cpp"
using namespace std;

void initCommandExe(int argc, char *argv[])
{
    Repository repo;
    if (!repo.initRepo())
        cout << "Repository Already exist.";

    repo.addFileToIndex("init.cpp");
    repo.addFileToIndex("add.cpp");
}

#endif
